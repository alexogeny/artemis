{
  "components": {
    "schemas": {
      "AuthenticationFlowResponse": {
        "properties": {
          "challenge": {
            "type": "object"
          },
          "credential_ids": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "fallback": {
            "type": "object"
          },
          "flow_token": {
            "type": "string"
          },
          "next": {
            "type": "object"
          },
          "provider": {
            "type": "object"
          },
          "session": {
            "type": "object"
          }
        },
        "required": [
          "flow_token",
          "next"
        ],
        "type": "object"
      },
      "CreateItem": {
        "properties": {
          "name": {
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      },
      "LoginStartRequest": {
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ],
        "type": "object"
      },
      "MfaAttempt": {
        "properties": {
          "code": {
            "type": "string"
          },
          "flow_token": {
            "type": "string"
          }
        },
        "required": [
          "flow_token",
          "code"
        ],
        "type": "object"
      },
      "PasskeyAttempt": {
        "properties": {
          "credential_id": {
            "type": "string"
          },
          "flow_token": {
            "type": "string"
          },
          "signature": {
            "type": "string"
          }
        },
        "required": [
          "flow_token",
          "credential_id",
          "signature"
        ],
        "type": "object"
      },
      "PasswordAttempt": {
        "properties": {
          "flow_token": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "flow_token",
          "password"
        ],
        "type": "object"
      }
    }
  },
  "info": {
    "title": "Demo API",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/__mere/auth/login/mfa": {
      "post": {
        "operationId": "bootstrap_login_mfa",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MfaAttempt"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationFlowResponse"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    },
    "/__mere/auth/login/passkey": {
      "post": {
        "operationId": "bootstrap_login_passkey",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PasskeyAttempt"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationFlowResponse"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    },
    "/__mere/auth/login/password": {
      "post": {
        "operationId": "bootstrap_login_password",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PasswordAttempt"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationFlowResponse"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    },
    "/__mere/auth/login/start": {
      "post": {
        "operationId": "bootstrap_login_start",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginStartRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationFlowResponse"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    },
    "/__mere/client.ts": {
      "get": {
        "operationId": "bootstrap_client",
        "responses": {
          "200": {
            "description": "Response"
          }
        }
      }
    },
    "/__mere/openapi.json": {
      "get": {
        "operationId": "bootstrap_openapi",
        "responses": {
          "200": {
            "description": "Response"
          }
        }
      }
    },
    "/__mere/ping": {
      "get": {
        "operationId": "bootstrap_ping",
        "responses": {
          "200": {
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    },
    "/items": {
      "post": {
        "operationId": "create_item",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateItem"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    },
    "/items/{item_id}": {
      "get": {
        "operationId": "get_item",
        "parameters": [
          {
            "in": "path",
            "name": "item_id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "object"
                  },
                  "type": "object"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    },
    "/ping": {
      "get": {
        "operationId": "get_ping",
        "responses": {
          "200": {
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            },
            "description": "Success"
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "https://{tenant}.{site}.{domain}",
      "variables": {
        "domain": {
          "default": "example.com"
        },
        "site": {
          "default": "demo"
        },
        "tenant": {
          "default": "public"
        }
      }
    }
  ]
}
